{
  "schemaVersion": 1,
  "id": "datafortress",
  "version": "${version}",
  "name": "Data Fortress",
  "description": "A little crazy CC:T addon that stores different data and, well, share it via internal http server!",
  "authors": [
    "SirEdvin"
  ],
  "accessWidener": "datafortress.accesswidener",
  "contact": {
    "homepage": "https://docs.siredvin.site/",
    "issues": "https://github.com/SirEdvin/DataFortress/issues",
    "sources": "https://github.com/SirEdvin/DataFortress"
  },
  "license": "MIT",
  "icon": "pack.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "site.siredvin.datafortress.FabricDataFortress"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "site.siredvin.datafortress.FabricDataFortressClient"
      }
    ],
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "site.siredvin.datafortress.data.FabricDataGenerators"
      }
    ]
  },
  "mixins": [
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": ">=1.20.1 <1.21",
    "java": ">=17",
    "computercraft": ">=${computercraftVersion}",
    "peripheralium": ">=${peripheraliumVersion}"
  }
}
